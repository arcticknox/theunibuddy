{
    "user": {
        "doc": "mongo db username",
        "format": "String",
        "default": "test",
        "env": "MONGO_DB_USER"
    },
    "password": {
        "doc":"mongo db password",
        "format": "String",
        "default": "test",
        "env": "MONGO_DB_PASSWORD"
    },
    "url": {
        "doc": "mongo db url",
        "format": "String",
        "default": "localhost:27017",
        "env": "MONGO_DB_URL"
    },
    "dbName": {
        "doc": "mongo db name",
        "format": "String",
        "default": "unibuddy",
        "env": "MONGO_DB_NAME"
    }
}